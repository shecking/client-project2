{{#each sessions as |session|}}
  {{log session}}
  <section data-id="{{session.id}}">
    <h6>Date:</h6>
    <h3>
      {{session.date}}
    </h3>
    <h6>Time:</h6>
    <h3>
      {{session.time}}
    </h3>
    <h6>Time Spent Practicing:</h6>
    <h3>
      {{session.practice_time}}
    </h3>
    <h6>Notes:</h6>
    <p>
      {{session.notes}}
    </p>
    <button type="submit" class="btn btn-primary edit-session" data-id="{{id}}">Edit</button>
    <button type="submit" class="btn btn-danger delete-session" data-id="{{id}}">Delete</button>
  </section>

{{/each}}
